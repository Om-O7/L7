
c program to check if the given number is even or odd

#include <stdio.h>
int main() {
    int num;
    printf("Enter an integer: ");
    scanf("%d", &num);

    if(num % 2 == 0)
        printf("%d is even.", num);
        printf("even")
    else
        printf("%d is odd.", num);
        printf("odd")
    return 0;
}
